<main class="main checkout">
    <!-- Start of Breadcrumb -->
    <nav class="breadcrumb-nav">
        <div class="container">
            <ul class="breadcrumb shop-breadcrumb bb-no">
                <li class="passed"><a href="/cart">Shopping Cart</a></li>
                <li class="active"><a href="/checkout">Checkout</a></li>
                <li><a href="javascript: void(0)">Order Complete</a></li>
            </ul>
        </div>
    </nav>
    <!-- End of Breadcrumb -->


    <!-- Start of PageContent -->
    <div class="page-content" id="container">
        <div class="container">
            <div class="row">
                <div class="col-md-12 d-flex justify-content-center">
                    <span class="border border-dark" style="padding: 61px; border: 1px solid;" >
                        <h2 style="margin: 0;">Thank You. Your Order has been Placed</h2>
                    </span>
                </div>
                <div class="row">
                    <div class="col-12 pt-4 pb-5">
                        <h3>Order Summery</h3>
                    </div>
                </div>
                    <div class="row align-items-center">
                        <div class="col-sm-6 mb-6">
                            <div class="ecommerce-address billing-address pr-lg-8">
                                <h4 class="title title-underline ls-25 font-weight-bold">Shipping Address</h4>
                                <address class="mb-4">
                                    <table class="address-table">
                                        <tbody>
                                            <tr>
                                                <th>Name:</th>
                                                <td><%=order.address.name%></td>
                                            </tr>
                                            <tr>
                                                <th>Address:</th>
                                                <td><%=order.address.address%></td>
                                            </tr>
                                            <tr>
                                                <th>Town:</th>
                                                <td><%=order.address.town%></td>
                                            </tr>
                                            <tr>
                                                <th>State:</th>
                                                <td><%=order.address.state%></td>
                                            </tr>
                                            <tr>
                                                <th>Postcode:</th>
                                                <td><%=order.address.zip%></td>
                                            </tr>
                                            <tr>
                                                <th>Phone:</th>
                                                <td><%=order.address.mobile%></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </address>
                            </div>
                        </div>
                        
                       
                    </div>
                    <div class="row">
                        <div class="col-lg-8">
                            <div class="cart-action mb-6" style="display: flex; flex-wrap: wrap;">
                                <a href="/" class="btn btn-dark btn-rounded btn-icon-left btn-shopping mr-auto"><i class="w-icon-long-arrow-left"></i>Continue Shopping</a>
                                <a href="/myaccount" class="btn btn-rounded btn-default btn-clear">View Orders</a>
                                
                            </div>
                        </div>
                    </div>
            </div>
            
        </div>
    </div>
    <!-- End of PageContent -->
</main>
<!-- End of Main -->